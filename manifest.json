{

    "name": "Chi Non Ha Risposto Al Sondaggio WhatsApp",
    "version": "0.1",
    "description": "Elenca le persone che non hanno risposto al sondaggio WhatsApp",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "64": "icons/64.png",
        "128": "icons/128.png",
        "256": "icons/256.png",
        "512": "icons/512.png"
    },
    "manifest_version": 3,
    "content_scripts": [{
        "js": [
            "script.js"
        ],
        "matches": [
            "https://web.whatsapp.com/"
        ]
    }],
    "web_accessible_resources": [{ 
        "resources": ["*.js"],
        "matches": ["<all_urls>"]
    }]
}
